#include <stdio.h>
#include <stdlib.h>

void MinMax(int *data, int *mi, int*ma){\\f(n)
     *mi = data[0];\\1
     *ma = data[0];\\1
     for(int i = 0; i < 10; i++){
         if(data[i] < *mi || data[i] > *ma) \\(n-1)
         *mi = data[i];
         *ma = data[i];
         }
}
int main(void){
    int data[10] = {22,09,33,28,13,17,01,44,90,11};
    int mi, ma;
    MinMax(data, &mi, &ma);
    printf("Valor minimo: %d\n", mi);
    printf("Valor maximo: %d\n", ma);
    exit(0);
}    
f(n) = (n-1)
